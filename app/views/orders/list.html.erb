<div class="row">
	
	<%= form_tag({:action => :upload}, :multipart => true ) do %>
	<div class="large-12 colum panel">
	<%= file_field_tag 'invoice' %>
	<%= submit_tag("Прочитать", class: "button success radius small" ) %>
	</div>
<% end %>
	
</div>



<div class="row">
  <div class="large-12 colum panel">
    <table class="table table-hover">
      <thead>
          <tr>
          <th>Номер</th>
          <th>Дата</th>
          <th>Сумма</th>
          <th>ИНН</th>
        </tr>
        <tbody>
          <% @docunemts.each do |docunemt| %>
            <tr>
              <td>
                <%= docunemt.number %>
              </td>
              <td>
                <%= docunemt.date %>
              </td>
              <td>
                <%= docunemt.sum %>
              </td>
              <td>
              	<span data-tooltip aria-haspopup="true" class="has-tip tip-left" title="ddddd">
                	<%= docunemt.inn %>
                </span>
              </td>
              <td>
                <%=  link_to 'Показать', documets_show_path(docunemt), method: :get %>
              </td>
            </tr>
          <% end %> 
        </tbody>
      </thead>

</table>
  </div>